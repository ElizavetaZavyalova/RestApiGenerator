{
  "ControllerName":{
    "dialect": "POSTGRES",
    "pseudonyms": {
      "tables": {
        "users": ["user","current_user"]
      }
    },
  "http": {
    "getEndpoint": {
      "type": "get:price|id|name",
      "request": "user/({i:eq_id}|[userFilter]&{b:is_name})/name/{i:id}/{s:nick}",
      "perms": true,
      "operation": "получить",
      "filters": {
        "userFilter:or": "s:eq_name|i:=age",
        "f3:and": "hello",
        "f2:sql": "name='Hello' or age=17",
        "f4:call": "name.age()"
      },
      "postEndpoint": {
        "request": "user/( {i:eq_id}|[userFilter]&{b:is_name} )/name/",
        "types": {
          "post": "age",
          "put":  "ahwe|jjdjd"
        },
        "filters": {
          "userFilter:or": "s:!=name|i:=age",
          "f3:and": "hello",
          "f2:sql": "name='Hello' or age=17",
          "f4:call": "name.age()"
        }
      },
      "deleteEndpoint": {
        "type": "delete",
        "request": "user/( {i:eq_id}|[userFilter]&{b:is_name} )/name/{s:id}/{s:nick}",
        "perms": true,
        "operation": "удалить",
        "filters": {
          "userFilter:or": "s:!=name|i:=age",
          "f3:and": "hello",
          "f2:sql": "name='Hello' or age=17",
          "f4:call": "name.age()"
        },
        "patchEndpoint": {
          "type": "patch:nick",
          "request": "user/( {i:eq_id}|[userFilter]&{b:is_name} )/name/{s:id}/{s:nick}",
          "perms": true,
          "operation": "изменить",
          "filters": {
            "userFilter:or": "s:!=name|i:=age",
            "f3:and": "hello",
            "f2:sql": "name='Hello' or age=17",
            "f4:call": "name.age()"
          }
        },
        "putEndpoint": {
          "type": "put:nick|name",
          "request": "user/( {i:eq_id}|[userFilter]&{b:is_name} )|[f2]/name/{s:id}/{s:nick}",
          "perms": true,
          "operation": "изменить целиком",
          "filters": {
            "userFilter:or": "s:!=name|i:=age",
            "f3:and": "hello",
            "f2:sql": "name='Hello' or age=17",
            "f4:call": "name.age()"
          }
        }
      }
    }
  }
  }
}