{
  "ControllerName": {
    "pseudonyms": {
      "tables": {
        "users": [
          "user",
          "current_user"
        ]
      }
    },
    "filters": {
      "userFilter:or": "s:eq_name|i:=age",
      "f3:and": "hello",
      "f2:sql": "name='Hello' or age=17",
      "f4:call": "name.age()"
    },
    "http": {
      "getEndpoint": {
        "type": "get:price|id|name",
        "request": "user/({i:eq_id}|[userFilter]&{b:is_name})/name/{i:id}/{s:nick}"
      },
      "postEndpoint": {
        "request": "user/( {i:eq_id}|[userFilter]&{b:is_name} )/name/",
        "types": {
          "post": "age",
          "put": "ahwe|jjdjd",
          "operation": "получить"
        }
      },
      "deleteEndpoint": {
        "type": "delete",
        "request": "user/( {i:eq_id}|[userFilter]&{b:is_name} )/name/{s:id}/{s:nick}"
      },
      "patchEndpoint": {
        "type": "patch:nick",
        "request": "user/( {i:eq_id}|[userFilter]&{b:is_name} )/name/{s:id}/{s:nick}"
      },
      "putEndpoint": {
        "type": "put:nick|name",
        "request": "user/( {i:eq_id}|[userFilter]&{b:is_name} )|[f2]/name/{s:id}/{s:nick}"
      }
    }
  }
}