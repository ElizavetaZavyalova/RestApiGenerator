{
  "pseudonyms": {
    "joins": {
      "users:reading": ["id", "reader_id"],
      "users:listening": ["id", "reader_id"],
      "listening:audio": ["audio_id", "id"],
      "audioBooks:audio": ["id","book_id"],
      "reading:books": ["book_id", "id"]
    },
    "tables": {
      "books": ["audioBooks"]
    },
    "entity": {
      "bookStop": ["LastPage-i=0"],
      "audioStop": ["LastChapter-i=0","MinuteIndex-i=0"],
      "book": ["BookName","Annotation","Year","pageCount","TextRef","author_id"],
      "audio": ["duration","audioRef","voiceover_id","book_id"],
      "audioBook": ["BookName","Annotation","author_id"]
    }
  },
  "http_prefix": "collection/",
  "http": {
    "Reading": {
      "post": "bookStop|user_id->id",
      "request": "books/{id}/reading"
    },
    "Listening": {
      "post": "audioStop|user_id->id",
      "request": "audio/{id}/listening"
    },
    "ReadingCollection": {
      "delete": "",
      "patch": "bookStop->id",
      "request": "audio/{id}/reading/{user_id}"
    },
    "ListeningCollection": {
      "delete": "",
      "patch": "audioStop->id",
      "request": "books/{id}/listening/{user_id}"
    },
    "ReadingCollectionOfBooks": {
      "get": "book|id->limit|sort",
      "request": "users/{id}/books"
    },
    "ListeningCollectionOfBooks": {
      "get": "audioBook|id->limit|sort",
      "request": "users/{id}/audioBooks"
    },
    "ListeningCollectionOfAudio": {
      "get": "audio|id->limit|sort",
      "request": "users/{id}/audio"
    }
  }
}