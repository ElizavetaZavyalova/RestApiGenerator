{
  "pseudonyms": {
    "tables": {
      "pseudonyms": ["!author","!voiceover"],
      "audio": ["audioBook"]
    },
    "entity": {
      "book": ["TextRef-s","BookName-s","Annotation-s","pageCount-i","year-i"],
      "audio": ["duration","audio_ref-s","book_id"]
    },
    "joins": {
      "author:books": ["id","author_id"],
      "books:audioBook": ["id","book_id"],
      "voiceover:audioBook": ["id","audio_id"]
    }
  },
  "http_prefix": "create/",
  "http": {
     "Book": {
       "post": "book->id",
       "delete": "",
       "get": "book|id->limit|sort",
       "request": "author/{id}/<books"
     },
    "BookById": {
      "delete": "",
      "get": "book|id->limit|sort",
      "patch": "book->id",
      "request": "books/{id}"
    },
    "AudioBook": {
      "post": "audio->id",
      "delete": "",
      "get": "audio|id->limit|sort",
      "request": "voiceover/{id}/<audioBook"
    },
    "AudioBookById": {
      "delete": "",
      "get": "audio|id->limit|sort",
      "patch": "audio->id",
      "request": "audioBook/{id}"
    }
  }
}